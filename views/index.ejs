<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')%>
<body>
    <%- include('partials/nav.ejs') %>
    
    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>
                <h3><%=blog.title %></h3>
                <p><%=blog.snippet %></p>
            <% }) %>
        <% } else { %>
            <p>No blog to display</p>
        <% } %>
    </div>
    <%- include('./partials/footer')%> 
</body>
</html>





